{
    "editor.fontFamily": "'Mononoki Nerd Font', Consolas, 'Courier New', monospace",
    "editor.fontSize": 16,
    "editor.lineHeight": 18,
    "editor.minimap.enabled": false,
    "editor.scrollbar.vertical": "hidden",
    "editor.overviewRulerBorder": false,
    "editor.hideCursorInOverviewRuler": true,
    "editor.formatOnSave": true,
    "editor.bracketPairColorization.enabled": true,
    "editor.stickyScroll.enabled": true,
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorBlinking": "expand",
    "editor.linkedEditing": true,
    "editor.guides.bracketPairs": true,
    "notebook.breadcrumbs.showCodeCells": false,
    "terminal.integrated.fontSize": 16,
    "python.formatting.provider": "none",
    "files.insertFinalNewline": true,
    "zenMode.centerLayout": true,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.sideBar.location": "right",
    "workbench.editor.showTabs": "single",
    "workbench.colorTheme": "Dracula",
    "workbench.preferredDarkColorTheme": "One Dark Pro Darker",
    "workbench.statusBar.visible": false,
    "git.openRepositoryInParentFolders": "never",
    "oneDarkPro.vivid": true,
    "oneDarkPro.bold": false,
    "editor.rulers": [
        80
    ],
    "editor.tokenColorCustomizations": {
        "[One Dark Pro Darker]": {
            "variables": "#ABB2BF",
            "textMateRules": [
                {
                    "scope": [
                        "keyword.operator",
                        "keyword.operator.arithmetic",
                        "keyword.operator.logical",
                        "keyword.operator.new",
                        "keyword.operator.comparision",
                    ],
                    "settings": {
                        "foreground": "#56B6C2",
                    },
                },
                {
                    "scope": [
                        "comment",
                        "keyword.control",
                        "keyword.other",
                        "storage.type.functions.js",
                        "storage.modifier.async.js",
                        "storage.type.js"
                    ],
                    "settings": {
                        "fontStyle": "italic"
                    }
                }
            ]
        }
    },
    "editor.semanticTokenColorCustomizations": {
        "enabled": true,
        "rules": {
            "*.mutable": {
                "underline": false
            }
        },
        "[One Dark Pro Darker]": {
            "rules": {
                "property": "#E06C75",
                "variable": "#ABB2BF",
            }
        }
    },
    "editor.fontLigatures": true,
    "[python]": {
        "editor.formatOnType": true,
        "editor.defaultFormatter": "ms-python.autopep8"
    },
    "security.workspace.trust.banner": "never",
    "workbench.editorAssociations": {
        "*.dll": "default"
    },
    "explorer.confirmDelete": false,
    "workbench.activityBar.location": "hidden",
    "chatgpt.lang": "en"
}
